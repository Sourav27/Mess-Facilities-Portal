<%if logged_in? %>

  <div class="page-header">
    <h1>Here is some info about you...</h1>
  </div>
  <div class="panel panel-default">
    <div class="panel-body">
      <ul>
        <li><strong>Name:</strong> <%= session[:username] %></li>
        <li><strong>uID:</strong> <%= session[:user_id] %></li>
        <li><img src="<%= session[:user_imageurl] %>"></li>
      </ul>
    </div>
  </div>

<%else%>
<%= link_to "sign in",  @auth_url %>
<%end%>

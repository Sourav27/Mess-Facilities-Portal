		<%= link_to 'Download Excel sheet', complaints_all_path(format: :xlsx, :from_date => params[:from_date], :to_date => params[:to_date]), target: "_blank", class: "btn btn-success pull-right" %>
		<table style="background: white;" class="table table-striped table-responsive">
		<thead>
			<th><strong>#</strong></th>
			<th><strong>Title</strong></th>
			<th><strong>Description</strong></th>
			<th><strong>Mess/Facility</strong></th>
			<th><strong>Category</strong></th>
			<th><strong>User</strong></th>
			<th><strong>Approached authorities?</strong></th>
			<th><strong>Date</strong></th>
			<th><strong>Resolved</strong></th>
		</thead>
		<tbody>
		<% i=1 %>
		<% @complaints.each do |complaint| %>
			<tr>
				<td><%= i %></td>
				<td><%= complaint.title %></td>
				<td><%= complaint.content %></td>
				<td><%= complaint.category.name %></td>
				<td><%= ComplaintCategory.find(complaint.complaint_category_ids).name %></td>
				<td><%= User.find_by(username: complaint.user_id).username %></td>
				<td><%= complaint.contacted %></td>
				<td><%= (complaint.created_at).strftime("%d-%m-%y") %></td>
				<td><%= complaint.solved %></td>
			</tr>
			<% i=i+1 %>
			<% end %>
		</tbody>
		</table>

	
